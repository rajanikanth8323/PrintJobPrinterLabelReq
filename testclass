    LabelPrinter_Detail__c printer = new LabelPrinter_Detail__c(
        Name = 'Printer1',
        Order_to_Company__c = 'Sri Lanka - Mainettech Lanka Pvt Ltd',
        PrintJob_Supplier__c = printsupli.Id
    );
    insert printer;

    Test.startTest();
